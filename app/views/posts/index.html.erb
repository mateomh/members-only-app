<p id="notice"><%= notice %></p>

<h1>Posts</h1>
<% if user_signed_in? %>
  <table>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td>
            <%= post.content %><br>
            <%= post.created_at %><br>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
<%= link_to 'New Post', new_post_path %>

<%= form_for current_user, url: "/users/sign_out", method: :delete do |f| %> 

    <button type="submit">Log Out</button>

<% end %>
<% else %>
  <%= link_to 'Sign in',  new_user_session_path %>
 <%= link_to 'Sign up',   new_user_registration_path %>

<% end %>

<br>






